    printf("digite o nome do carro: ");
    scanf("%s", carro);
    
    printf("digite a velocidade: ");
    scanf("%d", &velocidade);
    
    if(velocidade>=100){
        printf("RÃ¡pido");
    } else {
        printf("Devagar");
    }
    return 0;
    
}
